<script>
window.ssCallbacks = window.ssCallbacks || [];
window.ssCallbacks.push(function () { 
	var windowEventEnum = require('settings/enums/events/window.event.enum'); 
	removeFdLane = function () { 
		if ($('#bezorgbundelbonus').length > 0) { 
			SS.EventTrack.rp('co-626-bbb'); 

			if ($('#CO-626-blok').length > 0) { 
				$('#freedeliveries').hide();
				var discountLabels = $('.discount-block__label');

				$.each(discountLabels, function () {
					var discountLabel = $(this); 
					var discountLabelText = discountLabel.text().toLowerCase(); 
					if (discountLabelText.match("^gratis bezorgen") || discountLabelText.match("^gratis bezorging")) {
						var bonusGroup = discountLabel.parents('.product-cardview--bonus-group:eq(0)');

						if (bonusGroup.length) {
							bonusGroup.remove();
						} else {
							discountLabel.remove();
						}
					}
				}); 
			} 
		} 
	};

	Backbone.mediator.channel(windowEventEnum.CHANNEL).subscribe(windowEventEnum.PAGE_RENDER_COMPLETE, removeFdLane);
});
</script>
